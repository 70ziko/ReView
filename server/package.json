{
  "name": "server",
  "version": "0.1.0",
  "description": "The Api for the ReView application",
  "main": "app.*.ts",
  "scripts": {
    "dev": "npx tsx --env-file=../.env --watch  src/app.dev.ts",
    "build": "tsc",
    "start": "node dist/app.dev.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "format": "prettier --log-level warn --write './**/*.{js,ts,jsx,tsx}'"
  },
  "author": "70ziko",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.8",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "prettier": "3.5.3"
  },
  "dependencies": {
    "@langchain/openai": "^0.4.4",
    "@types/ws": "^8.5.14",
    "arangojs": "^10.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "imgur": "^2.4.3",
    "langchain": "^0.3.19",
    "langsmith": "^0.3.11",
    "morgan": "^1.10.0",
    "openai": "^4.86.1",
    "serpapi": "^2.1.0",
    "socket.io": "^4.8.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "ws": "^8.18.1",
    "zod": "^3.24.2"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.(ts|tsx)$": "babel-jest"
    },
    "extensionsToTreatAsEsm": [
      ".ts",
      ".tsx"
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
